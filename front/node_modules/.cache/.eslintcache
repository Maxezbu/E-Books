      color=\"secondary\"\n        onClick={handleClick}\n      >\n        Categorias\n      </Button>\n      <StyledMenu\n        id=\"customized-menu\"\n        anchorEl={anchorEl}\n        keepMounted\n        open={Boolean(anchorEl)}\n        onClose={handleClose}\n      >\n        {categorias &&\n          categorias.map((categoria, i) => {\n            return (\n              <StyledMenuItem\n                key={i}\n                onClick={() => searchCategory(categoria._id)}\n              >\n                <ListItemIcon>\n                  <StarHalfIcon fontSize=\"small\" />\n                </ListItemIcon>\n                <ListItemText primary={categoria.name} />\n              </StyledMenuItem>\n            );\n          })}\n      </StyledMenu>\n    </div>\n  );\n}\n","/home/buru/bootcamp/E-Books/front/src/utils/stylesLogin.js",[],"/home/buru/bootcamp/E-Books/front/src/utils/stylesAdminUsers.js",[],{"ruleId":"191","replacedBy":"192"},{"ruleId":"193","replacedBy":"194"},{"ruleId":"195","severity":1,"message":"196","line":15,"column":31,"nodeType":"197","messageId":"198","endLine":15,"endColumn":37},{"ruleId":"195","severity":1,"message":"199","line":34,"column":19,"nodeType":"197","messageId":"198","endLine":34,"endColumn":29},{"ruleId":"200","severity":1,"message":"201","line":71,"column":6,"nodeType":"202","endLine":71,"endColumn":8,"suggestions":"203"},{"ruleId":"200","severity":1,"message":"204","line":73,"column":6,"nodeType":"202","endLine":73,"endColumn":15,"suggestions":"205"},{"ruleId":"200","severity":1,"message":"206","line":20,"column":6,"nodeType":"202","endLine":20,"endColumn":8,"suggestions":"207"},{"ruleId":"200","severity":1,"message":"206","line":59,"column":6,"nodeType":"202","endLine":59,"endColumn":8,"suggestions":"208"},{"ruleId":"195","severity":1,"message":"209","line":3,"column":17,"nodeType":"197","messageId":"198","endLine":3,"endColumn":26},{"ruleId":"195","severity":1,"message":"210","line":5,"column":10,"nodeType":"197","m